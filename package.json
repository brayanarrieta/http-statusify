{
  "name": "http-statusify",
  "version": "1.0.0",
  "description": "Package includes all Official HTTP Response Status Codes",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "commit": "cz",
    "clean": "del ./dist/*",
    "build": "npm run clean && tsc",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "generator": "ts-node ./scripts/generator"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brayanarrieta/http-statusify.git"
  },
  "keywords": [
    "node",
    "client",
    "http",
    "HttpStatus",
    "status",
    "codes",
    "browser"
  ],
  "author": "brayanarrieta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brayanarrieta/http-statusify/issues"
  },
  "homepage": "https://github.com/brayanarrieta/http-statusify#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "ts-morph": "^13.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
